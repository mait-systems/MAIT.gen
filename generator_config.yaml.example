# Generator Configuration File - Template
# Copy this file to generator_config.yaml and update with your specific values
# Values here override the shared defaults in config/generator_base.yaml.

extends: config/generator_base.yaml

site:
  name: "default"

connection:
  host: "192.168.127.254"
  port: 502
  unit_id: 1
  retries: 5
  retry_delay: 0.1

influxdb:
  url: "http://influxdb:8086"
  org: "mlr"
  bucket: "stbd_gen"

logging:
  filename: "generator_output.log"
